{
    "json.schemas": [
        {
            "url": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
            "fileMatch": [
                "bucket/**/*.json"
            ]
        }
    ],
    "markdownlint.config": {
        "default": true,
        "MD013": false,
        "MD033": false
    },
    "sortJSON.orderOverride": [
        "##",
        "version",
        "description",
        "homepage",
        "license",
        "identifier",
        "notes",
        "depends",
        "suggest",
        "architecture",
        "64bit",
        "32bit",
        "arm64",
        "innosetup",
        "mode",
        "url",
        "jp",
        "jsonpath",
        "xpath",
        "re",
        "regex",
        "find",
        "replace",
        "reverse",
        "hash",
        "extract_dir",
        "extract_to",
        "pre_install",
        "installer",
        "file",
        "script",
        "args",
        "keep",
        "post_install",
        "env_add_path",
        "env_set",
        "psmodule",
        "bin",
        "shortcuts",
        "persist",
        "pre_uninstall",
        "uninstaller",
        "post_uninstall",
        "checkver",
        "github",
        "sourceforge",
        "autoupdate"
    ],
    "sortJSON.contextMenu": {
        "sortJSONAlphaNum": false,
        "sortJSONAlphaNumReverse": false,
        "sortJSONKeyLength": false,
        "sortJSONKeyLengthReverse": false,
        "sortJSONType": false,
        "sortJSONTypeReverse": false,
        "sortJSONValues": false,
        "sortJSONValuesReverse": false
    }
}
